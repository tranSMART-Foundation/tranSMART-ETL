    <sql>select samp.study_id
,samp.cohorts as COHORT_ID
,cast(sm.assay_id as varchar2(255)) as EXP_ID
from rwg_samples samp
inner join deapp.de_subject_sample_mapping sm
on  samp.expr_id = sm.sample_cd
and samp.study_id = sm.trial_name
where samp.study_id = '${STUDY_ID}'
order by samp.study_id
,samp.cohorts
,exp_id

